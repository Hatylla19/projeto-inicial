algoritmo "semnome"
var
   c, f, n:inteiro
inicio
      escreva ("informe um numero: ")
      leia(n)
      se(n = 0)entao
           escreva(1)
                senao
                f <- n
                c <- 1
      enquanto (c < n)faca
                f <- f*(n - c)
                c <- c + 1
      fimenquanto
      escreva(f)
      fimse
fimalgoritmo




questão 3:algoritmo "fibonacci"

var
   x, atual, anterior, novo:inteiro
inicio

   atual <- 1
   anterior <- 0
   escreval (atual)
   para x <- 1 ate 20 faca
      novo <- atual + anterior
      anterior <- atual
      atual <- novo
      escreval (novo)
      
   fimpara
   
fimalgoritmo



