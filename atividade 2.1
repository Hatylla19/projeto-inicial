Algoritmo "algoritmo que para gerar o número H"

Var
   H: REAL
   CONT,N:INTEIRO

Inicio
      ESCREVAL ("DIGITE N ")
      LEIA(N)
      CONT <- N
      ENQUANTO CONT <= N FACA
      H <- H + 1/CONT
      CONT <- CONT + 1
      FIMENQUANTO
      ESCREVA(H)

Fimalgoritmo




